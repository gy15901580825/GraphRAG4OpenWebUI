      <data key="d2">ef497a30bdb1d7f5e4ea4aa9acf66f17</data>
    </node>
    <node id="DEBUG OUTPUT">
      <data key="d0">LOGGING CONFIGURATION</data>
      <data key="d1">Debug output refers to the detailed logging information generated by network devices, which can help in diagnosing issues and understanding traffic behavior.</data>
      <data key="d2">ef497a30bdb1d7f5e4ea4aa9acf66f17</data>
    </node>
    <node id="MULTI-STAGE COS">
